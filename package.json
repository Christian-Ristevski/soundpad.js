{
  "name": "soundpad.js",
  "version": "1.2.4",
  "description": "An npm package for interacting with Soundpad.",
  "main": "lib/index.js",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js"
    },
    "./web": {
      "types": "./lib/web.d.ts",
      "import": "./lib/web.js"
    }
  },
  "type": "module",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "test": "echo \"Tests are not implemented yet\"",
    "doc": "typedoc src/index.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run lint:fix && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Seblor/soundpad.js"
  },
  "keywords": [
    "soundpad",
    "automation"
  ],
  "author": "Seblor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Seblor/soundpad.js/issues"
  },
  "homepage": "https://github.com/Seblor/soundpad.js#readme",
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@types/xml2js": "^0.4.11",
    "@typescript-eslint/eslint-plugin": "5.46.0",
    "@typescript-eslint/parser": "5.47.1",
    "eslint": "8.30.0",
    "eslint-plugin-jest": "27.1.7",
    "jest": "29.3.1",
    "prettier": "2.8.1",
    "ts-jest": "29.0.3",
    "ts-standard": "^12.0.2",
    "typedoc": "^0.23.25",
    "typedoc-plugin-missing-exports": "^1.0.0",
    "typedoc-plugin-rename-defaults": "^0.6.4",
    "typescript": "4.9.4"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "fast-xml-parser": "^4.1.2"
  },
  "ts-standard": {
    "project": "./tsconfig.json"
  }
}
